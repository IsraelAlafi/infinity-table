{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/index.js",
  "scripts": {
    "build": "webpack --mode production",
    "serve": "npm run build && node src/server/app.ts",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/app.ts",
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "make_copy": "rm -rf ~/git/react-node-boilerplate-for-interviews-master; mkdir ~/git/react-node-boilerplate-for-interviews-master ; cp -r ~/git/boilerplate_bak ~/git/react-node-boilerplate-for-interviews-master ; cd ~/git/react-node-boilerplate-for-interviews-master/boilerplate_bak ; npm install"
  },
  "author": "Sandeep Raveesh",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.3",
    "react": "^16.12.0",
    "react-dom": "^16.5.2",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "7.10.4",
    "babel-eslint": "^10.0.0",
    "babel-loader": "^8.1.0",
    "ts-node": "8.10.2",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.0.0",
    "css-loader": "^2.0.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^3.0.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.3",
    "prettier": "1.19.1",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3",
    "axios": "0.21.1",
    "react-bootstrap": "1.3.0",
    "bootstrap": "4.5.0",
    "sass": "1.26.10",
    "sass-loader": "9.0.2",
    "typescript": "^3.7.5",
    "mongoose": "^5.0.7"
  }
}
